requires 1.3.5

convert light -out=../.process
cd ../.process
calibrate light -dark=$defdark -flat=$defflat -cc=dark

register pp_light -2pass
seqapplyreg pp_light -drizzle -scale=1.4 -pixfrac=0.7 -framing=min

stack r_pp_light rej 3 3  -norm=addscale -output_norm -weight=wfwhm -filter-wfwhm=90% -out=../../master_light_$FILTER:%s$
