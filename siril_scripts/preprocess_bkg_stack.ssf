requires 1.2.0

convert light -out=../.process
cd ../.process

calibrate light -dark=../master_dark_ISO$ISOSPEED:%d$_EXP$EXPTIME:%.3f$.fit -flat=/Users/joydeepbiswas/Astrophotography/masters/master_flat_ISO$ISOSPEED:%d$ -cc=dark -cfa -debayer

seqsubsky pp_light 2 

# Align lights
register bkg_pp_light

# Stack calibrated lights to result.fit
stack r_bkg_pp_light rej 3 3 -norm=addscale -output_norm -rgb_equal -filter-fwhm=90% -out=bkg_result

load bkg_result