requires 1.2.0

convert lrgb -out=.process
cd .process
seqplatesolve lrgb -force
register lrgb -2pass
seqapplyreg lrgb -framing=min
rgbcomp -lum=r_lrgb_00003 r_lrgb_00004 r_lrgb_00002 r_lrgb_00001 -out=composed_lrgb
load composed_lrgb
subsky 1
save composed_lrgb

